<%if @may_pay[0]%>
  $("#cash_reload").html('<%= j(render :partial => "cash") %>');
  $("#car_nums").html('<%= j(render :partial => "car_num") %>');
  <%if @orders.length > 0 %>
    change_order(<%= params[:store_id] %>);
  <%else%>
    $("#tab_pay .close").trigger("click");
  <%end%>
  <%unless @may_pay[2].blank? %>
    <% if params[:pay_order][:is_print].to_i == 1 %>
      window.open('/stores/'+<%=params[:store_id]%>+'/set_stores/print_paper?c_id=<%=params[:customer_id]%>&n_id=<%=params[:car_num_id]%>&o_id=<%=@may_pay[2].join(',')  %>','_blank','height=520,width=625,left=10,top=100');
    <%end%>
    <% if params[:pay_order][:is_order].to_i == 1  %>
      window.open('/stores/'+<%=params[:store_id]%>+'/set_stores/three_line_print?c_id=<%=params[:customer_id]%>&n_id=<%=params[:car_num_id]%>&o_id=<%=@may_pay[2].join(',')  %>','_blank','height=585,width=825,left=10,top=100');
    <%end%>
  <%end%>
<%end%>
tishi_alert("<%=@may_pay[1]%>");



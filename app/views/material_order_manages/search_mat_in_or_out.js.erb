<% if @status == 0 %>
  tishi_alert("查询成功！");
<% elsif @status == 1%>
  tishi_alert("开始日期和结束日期均不能为空！");
<% elsif @status == 2%>
  tishi_alert("开始日期不能大于结束日期！");
<% end %>
<% if @mat_in_or_out == "ruku" %>
    $("#mat_out_list").hide();
    $("#mat_in_list").html('<%= escape_javascript(render :partial => '/material_order_manages/mat_in_list') %>');
    $("#mat_in_list").show();
<% elsif @mat_in_or_out == "chuku"%>
    $("#mat_in_list").hide();
    $("#mat_out_list").html('<%= escape_javascript(render :partial => '/material_order_manages/mat_out_list') %>');
    $("#mat_out_list").show();
<%end%>
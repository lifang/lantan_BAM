<div class="main">
  <div class="box">
    <div class="data_box">
      <div class="tab_head">
        <ul>
          <li onclick="window.location.href='/stores/<%=params[:store_id]%>/finance_reports/'">主营收入</li>
          <li>其他收入</li>
          <li  class="hover">收入报表</li>
        </ul>
      </div>
      <div class="data_body">
        <div>
          <div class="search">
            <div><label>时间：</label><input id="c_first" type="text"  onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                                          value="<%=@start_time%>"/> 到
              <input id="c_last" type="text" onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                     value="<%=@end_time%>" /></div>
            <div><button class="search_btn" onclick="revenue_report(this,<%=params[:store_id]%>)">查询</button></div>
            <div class="data_menu afterClear"><a href="javascript:void(0)">打印</a></div>
          </div>
          <div class="data_item" id="cash_over">
            <%= render 'report' %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>